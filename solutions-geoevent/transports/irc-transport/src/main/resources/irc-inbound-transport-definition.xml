<transport name="IrcInboundTransport" label="${com.esri.geoevent.solutions.transport.irc.irc-transport.TRANSPORT_INBOUND_LABEL}" contact="cbailey@esri.com" version="10.4.0" domain="com.esri.geoevent.solutions.transport.irc.inbound" type="inbound">
