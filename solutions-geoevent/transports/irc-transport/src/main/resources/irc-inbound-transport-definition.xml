		<propertyDefinition propertyName="channelList" label="${com.esri.geoevent.solutions.transport.irc.irc-transport.LBL_CHANNEL_LIST}"
			description="${com.esri.geoevent.solutions.transport.irc.irc-transport.DESC_CHANNEL_LIST}"
